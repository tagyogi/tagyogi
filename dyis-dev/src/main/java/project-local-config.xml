    <devInit>
		<oracle>
			<devDbMas>
				<table>
					CREATE TABLE DEV_DB_MAS
					(
					  TBL_SN   NUMBER(10)                           NOT NULL/
					  TBL_NM   VARCHAR2(4000 BYTE)                  NOT NULL/
					  TBL_CMT  VARCHAR2(1000 BYTE)/
					  USE_AT   CHAR(1 BYTE)
					)
				</table>
				<index>
					CREATE UNIQUE INDEX IDX_DEV_DB_MAS_PK ON DEV_DB_MAS(TBL_SN)
				</index>
				<pk>
					ALTER TABLE DEV_DB_MAS ADD (
					  CONSTRAINT IDX_DEV_DB_MAS_PK
