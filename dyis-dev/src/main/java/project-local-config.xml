				</pk>

			</devDbMas>
			<devDbDtl>
				<table>
					CREATE TABLE DEV_DB_DTL
					(
					  TBL_SN    NUMBER(10)                          NOT NULL/
					  COL_SN    NUMBER(10)                          NOT NULL/
					  COL_NM    VARCHAR2(50 BYTE)/
					  COL_CMT   VARCHAR2(1000 BYTE)/
					  COL_TYPE  VARCHAR2(1000 BYTE)/
					  COL_LEN   NUMBER(10)/
					  COL_PRE   NUMBER(5)/
					  PK_AT     CHAR(1 BYTE)                        DEFAULT '0'/
					  NULL_AT   CHAR(1 BYTE)                        DEFAULT '0'/
					  DEF_VAL   VARCHAR2(4000 BYTE)/
					  RMK       VARCHAR2(4000 BYTE)
					)
				</table>
				<index>
					CREATE UNIQUE INDEX IDX_DEV_DB_DTL_PK ON DEV_DB_DTL(TBL_SN/ COL_SN)
				</index>
				<pk>
					ALTER TABLE DEV_DB_DTL ADD (
					  CONSTRAINT IDX_DEV_DB_DTL_PK
					  PRIMARY KEY
					  (TBL_SN/ COL_SN)
					  USING INDEX IDX_DEV_DB_DTL_PK
					  ENABLE VALIDATE)
				</pk>

			</devDbDtl>
